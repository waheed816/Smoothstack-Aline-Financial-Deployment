{
  "name": "admin-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@hookform/resolvers": "^2.8.0",
    "@stomp/stompjs": "^6.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0",
    "dotenv": "^10.0.0",
    "jwt-decode": "^3.1.2",
    "sass": "^1.22.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.15.3",
    "react-query": "^3.25.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "sweetalert2": "^11.1.5",
    "sweetalert2-react-content": "^4.1.1",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:noWatch": "npm run test -- --watchAll=false",
    "test:coverage": "react-scripts test --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "test:lintReport": "eslint -f json -o eslint-report.json .",
    "test:CI": "npm run test:coverage && npm run test:lintReport && npm run sonar",
    "eject": "react-scripts eject",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "quotes": [
        "warn",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "build"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gulp-sass": "^5.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "react-styleguidist": "^11.1.7",
    "sonarqube-scanner": "^2.8.1"
  }
}
